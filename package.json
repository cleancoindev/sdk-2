{
  "version": "0.1.5-beta",
  "name": "@neptunemutual/sdk",
  "author": "Neptune Mutual Team",
  "description": "Neptune Mutual SDK",
  "license": "MIT",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "source": "src/index.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "default": "./dist/index.modern.js"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.esm.js",
  "unpkg": "./dist/index.umd.js",
  "scripts": {
    "build": "microbundle",
    "dev": "microbundle watch",
    "standard": "ts-standard",
    "fix": "ts-standard --fix"
  },
  "size-limit": [
    {
      "path": "dist/sdk.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/sdk.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "bs58": "4.0.1",
    "ethers": "5.4.1",
    "ipfs-mini": "1.1.5",
    "lru-cache": "6.0.0"
  },
  "devDependencies": {
    "@types/bs58": "4.0.1",
    "@types/lru-cache": "5.1.1",
    "microbundle": "0.14.2",
    "ts-standard": "11.0.0"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neptune-mutual/sdk.git"
  },
  "bugs": {
    "url": "https://github.com/neptune-mutual/sdk/issues"
  },
  "homepage": "https://github.com/neptune-mutual/sdk#readme"
}